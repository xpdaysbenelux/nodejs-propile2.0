{
    "name": "propile2",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "NODE_ENV=local nest start --watch",
        "start:dev:clean": "yarn db:drop && yarn migrate && yarn seed && yarn start:dev",
        "start:debug": "NODE_ENV=local nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "format:check": "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:fix": "prettier -c  --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "test": "jest",
        "docker:start": "docker-compose -f ./docker/docker-compose.yml up -d",
        "docker:stop": "docker-compose -f ./docker/docker-compose.yml stop",
        "migrate": "yarn typeorm migration:run -f ./data/ormconfig-cli.js",
        "migrate:revert": "yarn typeorm migration:revert -f ./data/ormconfig-cli.js",
        "migration:generate": "yarn typeorm migration:generate -f ./data/ormconfig-cli.js",
        "typeorm": "ts-node ./node_modules/typeorm/cli.js",
        "db:drop": "yarn typeorm schema:drop -f ./data/ormconfig-cli.js",
        "db:rollup": "yarn db:drop && yarn migrate",
        "seed": "yarn typeorm migration:run -f ./data/ormconfig-cli.js -c seeds",
        "seed:generate": "yarn typeorm migration:create -f ./data/ormconfig-cli.js -c seeds",
        "seed:revert": "yarn typeorm migration:revert -f ./data/ormconfig-cli.js -c seeds",
        "seed:prod": "yarn typeorm migration:run -f ./data/ormconfig-cli.js -c seeds-prod",
        "seed:prod:generate": "yarn typeorm migration:create -f ./data/ormconfig-cli.js -c seeds-prod",
        "seed:prod:revert": "yarn typeorm migration:revert -f ./data/ormconfig-cli.js -c seeds-prod",
        "seed:all": "yarn seed && yarn seed:prod",
        "db:rollup:seed": "yarn db:rollup && yarn seed:all"
    },
    "dependencies": {
        "@nestjs/common": "^6.11.7",
        "@nestjs/core": "^6.11.7",
        "@nestjs/jwt": "^6.1.1",
        "@nestjs/passport": "^6.1.1",
        "@nestjs/platform-express": "^6.11.7",
        "@nestjs/swagger": "^4.2.8",
        "@nestjs/typeorm": "^6.3.1",
        "@sentry/node": "^5.12.3",
        "bcrypt": "^3.0.8",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "compression": "^1.7.4",
        "connect-redis": "^4.0.4",
        "dotenv-safe": "^8.2.0",
        "express-rate-limit": "^5.1.1",
        "express-rate-limiter": "^1.3.1",
        "express-session": "^1.17.0",
        "helmet": "^3.21.2",
        "mandrill-api": "^1.0.45",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "pg": "^7.18.1",
        "ramda": "^0.27.0",
        "redis": "^3.0.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "ts-mockito": "^2.5.0",
        "typeorm": "^0.2.22",
        "uuid": "^3.4.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^6.14.2",
        "@nestjs/schematics": "^6.9.4",
        "@nestjs/testing": "^6.11.7",
        "@types/bcrypt": "^3.0.0",
        "@types/compression": "^1.7.0",
        "@types/connect-redis": "^0.0.13",
        "@types/dotenv-safe": "^8.1.0",
        "@types/express": "^4.17.2",
        "@types/express-rate-limit": "^3.3.3",
        "@types/express-session": "^1.15.16",
        "@types/faker": "^4.1.9",
        "@types/helmet": "^0.0.45",
        "@types/jest": "^25.1.2",
        "@types/mandrill-api": "^1.0.30",
        "@types/node": "^13.7.1",
        "@types/passport": "^1.0.2",
        "@types/passport-local": "^1.0.33",
        "@types/ramda": "^0.26.41",
        "@types/redis": "^2.8.15",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^3.4.7",
        "@typescript-eslint/eslint-plugin": "^2.19.2",
        "@typescript-eslint/parser": "^2.19.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "faker": "^4.1.0",
        "jest": "^25.1.0",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "swagger-ui-express": "^4.1.3",
        "ts-jest": "^25.2.0",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
