{
    "name": "silvernext",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "NODE_ENV=local nest start --watch",
        "start:dev:clean": "yarn db:drop && yarn migrate && yarn seed && yarn start:dev",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "format:check": "prettier -c \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:fix": "prettier -c  --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "docker:start": "docker-compose -f ./docker/docker-compose.yml up -d",
        "docker:stop": "docker-compose -f ./docker/docker-compose.yml stop",
        "migrate": "yarn typeorm migration:run -f ./data/ormconfig-cli.js",
        "migrate:revert": "yarn typeorm migration:revert -f ./data/ormconfig-cli.js",
        "migration:generate": "yarn typeorm migration:generate -f ./data/ormconfig-cli.js",
        "typeorm": "ts-node ./node_modules/typeorm/cli.js",
        "db:drop": "yarn typeorm schema:drop -f ./data/ormconfig-cli.js",
        "db:rollup": "yarn db:drop && yarn run migrate",
        "seed": "yarn run typeorm migration:run -f ./data/ormconfig-cli.js -c seeds",
        "seed:generate": "yarn run typeorm migration:create -f ./data/ormconfig-cli.js -c seeds",
        "seed:revert": "yarn run typeorm migration:revert -f ./data/ormconfig-cli.js -c seeds"
    },
    "dependencies": {
        "@nestjs/common": "^6.7.2",
        "@nestjs/core": "^6.7.2",
        "@nestjs/jwt": "^6.1.1",
        "@nestjs/passport": "^6.1.1",
        "@nestjs/platform-express": "^6.7.2",
        "@nestjs/swagger": "^4.1.7",
        "@nestjs/typeorm": "^6.2.0",
        "@sentry/node": "^5.10.2",
        "bcrypt": "^3.0.7",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.0",
        "compression": "^1.7.4",
        "connect-redis": "^4.0.3",
        "dotenv-safe": "^8.2.0",
        "express-rate-limit": "^5.0.0",
        "express-rate-limiter": "^1.3.1",
        "express-session": "^1.17.0",
        "helmet": "^3.21.2",
        "mandrill-api": "^1.0.45",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "pg": "^7.17.0",
        "ramda": "^0.26.1",
        "redis": "^2.8.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.0",
        "rxjs": "^6.5.3",
        "ts-mockito": "^2.5.0",
        "typeorm": "^0.2.22",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@nestjs/cli": "^6.9.0",
        "@nestjs/schematics": "^6.7.0",
        "@nestjs/testing": "^6.10.13",
        "@types/bcrypt": "^3.0.0",
        "@types/compression": "^1.0.1",
        "@types/connect-redis": "^0.0.13",
        "@types/dotenv-safe": "^8.1.0",
        "@types/express": "^4.17.1",
        "@types/express-rate-limit": "^3.3.3",
        "@types/express-session": "^1.15.16",
        "@types/faker": "^4.1.8",
        "@types/helmet": "^0.0.45",
        "@types/jest": "^24.0.18",
        "@types/mandrill-api": "^1.0.30",
        "@types/node": "^12.7.5",
        "@types/passport": "^1.0.2",
        "@types/passport-local": "^1.0.33",
        "@types/ramda": "^0.26.39",
        "@types/redis": "^2.8.14",
        "@types/supertest": "^2.0.8",
        "@types/uuid": "^3.4.6",
        "@typescript-eslint/eslint-plugin": "^2.14.0",
        "@typescript-eslint/parser": "^2.14.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.9.0",
        "faker": "^4.1.0",
        "jest": "^24.9.0",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "swagger-ui-express": "^4.1.2",
        "ts-jest": "^24.1.0",
        "ts-loader": "^6.1.1",
        "ts-node": "^8.5.4",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
